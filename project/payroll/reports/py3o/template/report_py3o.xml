        <field name="report_type">py3o</field>
        <field name="py3o_filetype">ods</field>
        <field name="module">payroll</field>
        <field name="binding_model_id" ref="model_payroll_wage_history" />
        <field name="binding_type">report</field>
        <field name="py3o_template_fallback">demo/payroll_wage_history_fallback.ods</field>
    </record>
</odoo>
