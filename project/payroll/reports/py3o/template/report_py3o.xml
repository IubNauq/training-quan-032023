<?xml version="1.0" encoding="utf-8"?>
<odoo>
     <record id="payroll_py3o_odt" model="ir.actions.report">
        <field name="name">Report Py3o Docx</field>
        <field name="model">payroll.wage.history</field>
        <field name="report_name">payroll.payroll_py3o_odt</field>
        <field name="report_type">py3o</field>
